<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="author" content="<%= @model['site']['author'] %>">
  <meta name="description" content="<%= @model['site']['description'] %>">
  <meta name="twitter:card" content="summary">
  <meta property="og:title" content="<%= @model['site']['name'] %>">
  <meta property="og:description" content="<%= @model['site']['description'] %>">
  <meta property="og:type" content="website">
  <meta property="og:url" content="<%= @model['site']['domain'] %><%= absolute_url %>">
  <meta property="og:image" content="<%= @model['site']['domain'] %>/asset/img/icon/favicon.png">
  <meta property="og:site_name" content="<%= @model['site']['name'] %>">
  <meta property="og:locale" content="<%= @model['site']['locale'] %>">
  <title><%= @model['site']['name'] %></title>
  <link rel="stylesheet" href="<%= href 'asset/css/abrascet.css' %>">
  <link rel="shortcut icon" href="<%= href 'asset/img/icon/favicon.png' %>">
</head>
